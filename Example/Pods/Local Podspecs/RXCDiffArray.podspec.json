{
  "name": "RXCDiffArray",
  "version": "1.0",
  "authors": {
    "ruixingchen": "rxc@ruixingchen.com"
  },
  "summary": "an array that can notify its changes",
  "description": "an array that can notify its changes to multi delegates",
  "homepage": "https://github.com/ruixingchen/RXCDiffArray",
  "license": "MIT",
  "source": {
    "git": "https://github.com/ruixingchen/RXCDiffArray.git",
    "tag": "1.0"
  },
  "requires_arc": true,
  "swift_versions": "5.0",
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": [
    "Core",
    "UIKitExtension"
  ],
  "subspecs": [
    {
      "name": "Core",
      "ios": {
        "source_files": "Source/*.swift",
        "frameworks": "Foundation"
      }
    },
    {
      "name": "UIKitExtension",
      "ios": {
        "source_files": "Source/Extension/UIKit/*.swift",
        "frameworks": [
          "Foundation",
          "UIKit"
        ]
      }
    },
    {
      "name": "ASDKExtension",
      "dependencies": {
        "Texture": [
          "> 2.8"
        ]
      },
      "ios": {
        "source_files": "Source/Extension/ASDK/*.swift",
        "frameworks": [
          "Foundation",
          "UIKit",
          "AsyncDisplayKit"
        ]
      }
    },
    {
      "name": "DifferenceKit",
      "dependencies": {
        "DifferenceKit": [
          "~> 1.1"
        ]
      },
      "ios": {
        "source_files": "Source/DifferenceKit/**/*.swift",
        "frameworks": [
          "Foundation",
          "DifferenceKit"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
